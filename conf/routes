GET        /                                  controllers.HomeController.index

GET        /websocket                         controllers.WebsocketController.connect()

GET        /admin                             controllers.AdminController.index
GET        /admin/test                        controllers.AdminController.test
GET        /admin/status                      controllers.AdminController.status
GET        /admin/trace/game/:id              controllers.AdminController.traceGame(id)
GET        /admin/trace/connection/:id        controllers.AdminController.traceConnection(id)

GET        /assets/*file                      controllers.Assets.at(path="/public", file)
